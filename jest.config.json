{
  "bail": true,
  "testEnvironment": "jsdom",
  "setupTestFrameworkScriptFile": "./testSetup.js",
  "snapshotSerializers": [
    "enzyme-to-json/serializer"
  ],
  "coveragePathIgnorePatterns": [
    "./.next/",
    "./coverage/"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 70,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  }
}